{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dtu"
		},
		"AdvWorksdb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksdb'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase2'"
		},
		"CustomerBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CustomerBlob'"
		},
		"CustomerTable_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CustomerTable'"
		},
		"ProductsTable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductsTable'"
		},
		"ProductBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductBlobStorage3'"
		},
		"AdvWorksdb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "newserver123.database.windows.net"
		},
		"AdvWorksdb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "newsql1"
		},
		"AdvWorksdb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dushyant"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "uniquename123.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql123hash"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dushyant"
		},
		"AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "newserver123.database.windows.net"
		},
		"AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "newsql1"
		},
		"AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dushyant"
		},
		"CustomerTable_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "newserver123.database.windows.net"
		},
		"CustomerTable_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "newsql1"
		},
		"CustomerTable_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dushyant"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dtu_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksdb_password": {
						"value": "[parameters('AdvWorksdb_password')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDatabase2_password": {
						"value": "[parameters('AzureSqlDatabase2_password')]"
					},
					"CustomerBlob_connectionString": {
						"value": "[parameters('CustomerBlob_connectionString')]"
					},
					"CustomerTable_password": {
						"value": "[parameters('CustomerTable_password')]"
					},
					"ProductsTable_connectionString": {
						"value": "[parameters('ProductsTable_connectionString')]"
					},
					"ProductBlobStorage3_connectionString": {
						"value": "[parameters('ProductBlobStorage3_connectionString')]"
					},
					"AdvWorksdb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksdb_properties_typeProperties_server')]"
					},
					"AdvWorksdb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksdb_properties_typeProperties_database')]"
					},
					"AdvWorksdb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksdb_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_userName')]"
					},
					"CustomerTable_properties_typeProperties_server": {
						"value": "[parameters('CustomerTable_properties_typeProperties_server')]"
					},
					"CustomerTable_properties_typeProperties_database": {
						"value": "[parameters('CustomerTable_properties_typeProperties_database')]"
					},
					"CustomerTable_properties_typeProperties_userName": {
						"value": "[parameters('CustomerTable_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}